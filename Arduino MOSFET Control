//very basic code to test out the arduino's ability to work through the MOSFET. Will greatly update with testing and when working with actual data

void setup() {
// initialize digital pin 3 as an output.
pinMode(3, OUTPUT);
}
// the loop function runs over and over again forever
void loop() {

 int v; //voltage of battery
 int thresh ; //low threshold
 
 //commented out code was to test LED control
/*digitalWrite(3, HIGH); // turn the LED on (HIGH is the voltage level)
delay(1000); // wait for a second
digitalWrite(3, LOW); // turn the LED off by making the voltage LOW
delay(1000);  // wait for a second */

if(v >= thresh){

  digitalWrite(3, HIGH);
  
}
else{

  digitalWrite(3, LOW); 
}
}

